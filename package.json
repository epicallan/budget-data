{
  "name": "budget-data",
  "version": "0.0.1",
  "description": "pdf scrapping",
  "main": "dist/app.js",
  "scripts": {
    "build": "rimraf dist && webpack --progress --colors ",
    "build-watch": "webpack --watch",
    "lint": "eslint src --cache --cache-location /home/allan/.eslintcache/",
    "test": " NODE_ENV=test ./node_modules/mocha/bin/mocha $(find src -name '*-test.js')  --compilers js:babel-core/register",
    "test-watch": "NODE_ENV=test ./node_modules/mocha/bin/mocha $(find src -name '*-test.js') --compilers js:babel-core/register --watch",
    "start": "NODE_ENV=production node dist/app.js",
    "dev": "concurrently --kill-others \"npm run build-watch\" \"nodemon dist/app.js\""
  },
  "repository": "github.com/epicallan/budget-data",
  "keywords": [
    "data",
    "mining",
    "data mining",
    "pdf",
    "development Initiative"
  ],
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "chai": "^3.2.0",
    "concurrently": "^2.1.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.13.0"
  },
  "author": {
    "name": "Allan",
    "email": "epicallan.al@gmail.com"
  },
  "engines": {
    "node": ">=5.6.0"
  },
  "license": "MIT",
  "dependencies": {
    "fast-csv": "^2.0.0",
    "verbal-expressions": "^0.2.1"
  }
}
